<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        <title>dev-повседневность</title>
        <link>http://podcast.ostretsov.ru/</link>
        <description>о повседневной практике разработчиков софта</description>
        <atom:link href="http://podcast.ostretsov.ru/feed/rss.xml" rel="self" type="application/rss+xml" />
        <language>ru</language>
        <lastBuildDate>{{ 'now'|date('D, d M y H:i:s O') }}</lastBuildDate>
        <managingEditor>artem@ostretsov.ru (Artem Ostretsov)</managingEditor>
        {% for page in pages[0:10] %}
            <item>
                <title>{{ page.title }}</title>
                <enclosure url="{{ page.audioURL }}" type="audio/mpeg" />
                <description>[CDATA[{{ page.content}}]]</description>
                <pubDate>{{ page.publishedAt|date('D, d M y H:i:s O') }}</pubDate>
                <guid>http://podcast.ostretsov.ru{{ page.path }}</guid>
            </item>
        {% endfor %}
    </channel>
</rss>
